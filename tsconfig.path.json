{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@domain/*": [
        "src/domain/*"
      ],
      "@infrastructure/*": [
        "src/infrastructure/*"
      ],
      "@resolvers/*": [
        "src/resolvers/*"
      ],
      "@root/*": [
        "./*"
      ],
      "@schemas/*": [
        "src/domain/schemas/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@transport/*": [
        "src/transport/*"
      ]
    }
  }
}
